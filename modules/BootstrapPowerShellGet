##AH:UseTextMode
module BootstrapPowerShellGet
{
    
    
    with executionPolicy=always {
        set $b = $PSEval("Install-Module PackageManagementProviderResource -Force  -AllowClobber -verbose|out-null; $true");
        set $a = $PSEval("Install-Module PackageManagement -Force  -AllowClobber -verbose|out-null; $true");
        
        # Must have this package available
        PSCall Install-PowerShellModule
        (
          ModuleName: PackageManagementProviderResource  
        );
        
        PSCall Install-PowerShellModule
        (
            ModuleName: PackageManagement
        );
        

    }
}
