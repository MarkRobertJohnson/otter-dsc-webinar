##AH:UseTextMode
module BootstrapPowerShellGet
{
    
    
    with executionPolicy=always {
        set $b = $PSEval("if(!(Get-DscResource -Module PackageManagementProviderResource -ErrorAction SilentlyContinue)) {Install-Module PackageManagementProviderResource -Force  -AllowClobber -verbose|out-null}; $true");
        set $a = $PSEval("if(!(Get-DscResource -Module PackageManagement -ErrorAction SilentlyContinue)) {Install-Module PackageManagement -Force  -AllowClobber -verbose|out-null}; $true");
      
    }
}
